{
  "name": "vite-react-auth-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "setup:env": "node -e \"const fs=require('node:fs'); if(!fs.existsSync('.env') && fs.existsSync('.env.example')) fs.copyFileSync('.env.example','.env');\"",
    "auth:ensure": "playwright-kit auth ensure --dotenv",
    "pretest:e2e": "npm run auth:ensure",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@playwright-kit/auth": "*",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "playwright": "^1.40.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.11"
  }
}
